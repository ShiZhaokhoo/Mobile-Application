<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Materials.MaterialDetails.FurtherDetail.MaterialFurtherDetailsFragment">

        <TextView
            android:id="@+id/background_rec"
            android:layout_width="347dp"
            android:layout_height="600dp"
            android:layout_marginStart="6dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            android:background="@drawable/rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="347dp"
            android:layout_height="600dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.562"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainGrid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:alignmentMode="alignMargins"
                android:columnCount="2"
                android:columnOrderPreserved="false"
                android:minHeight="480dp"
                android:padding="5dp"
                android:rowCount="2"
                tools:layout_editor_absoluteX="0dp">

                <Button
                    android:id="@+id/restockConfirmButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="100dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/button"
                    android:text="Confirm"
                    app:backgroundTint="@null"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/receiverEdit" />

                <TextView
                    android:id="@+id/partNumberEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/serialNumberEdit" />

                <TextView
                    android:id="@+id/serialNumberEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/receiverEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/receiveDateEdit" />

                <TextView
                    android:id="@+id/receiveDateEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/rackEdit" />

                <TextView
                    android:id="@+id/statusEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/remainingQuantityEdit" />

                <TextView
                    android:id="@+id/rackEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/statusEdit" />

                <TextView
                    android:id="@+id/remainingQuantityEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/issuesQuantityEdit" />

                <TextView
                    android:id="@+id/issuesQuantityEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/returnQuantityEdit" />

                <TextView
                    android:id="@+id/returnQuantityEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/receivedQuantityEdit" />

                <TextView
                    android:id="@+id/receivedQuantityEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="-"
                    android:textSize="16sp"
                    app:layout_constraintStart_toEndOf="@+id/serialNumberText"
                    app:layout_constraintTop_toBottomOf="@+id/partNumberEdit" />

                <TextView
                    android:id="@+id/statusText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Status :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/remainingQuantityText" />

                <TextView
                    android:id="@+id/restockRackIdText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Rack ID :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/statusText" />

                <TextView
                    android:id="@+id/remainingQuantityText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Remaining Quantity :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/issuesQuantityText" />

                <TextView
                    android:id="@+id/issuesQuantityText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Issues Quantity :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/returnQuantityText" />

                <TextView
                    android:id="@+id/returnQuantityText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Return Quantity :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/receivedQuantityText" />

                <TextView
                    android:id="@+id/receivedQuantityText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Received Quantity :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/restockPartNumberText" />

                <TextView
                    android:id="@+id/receivedDateText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Received Date :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/restockRackIdText" />

                <TextView
                    android:id="@+id/receiverText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Receiver :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/receivedDateText" />

                <TextView
                    android:id="@+id/restockPartNumberText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Part Number :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/serialNumberText" />

                <TextView
                    android:id="@+id/serialNumberText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:fontFamily="@font/montserrat"
                    android:text="Serial Number :       "
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>